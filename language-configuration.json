{
  "comments": {
    "lineComment": "#"
  },
  "brackets": [["[", "]"]],
  "autoClosingPairs": [
    ["[", "]"],
    ["\"", "\""],
    ["'", "'"]
  ],
  "surroundingPairs": [
    ["[", "]"],
    ["\"", "\""],
    ["'", "'"]
  ],
  "folding": {
    "markers": {
      "start": "^\\s*\\[",
      "end": "^\\s*(?=\\[|$)"
    }
  },
  "wordPattern": "(-?\\d*\\.\\d\\w*)|([^\\`\\~\\!\\@\\#\\%\\^\\&\\*\\(\\)\\-\\=\\+\\[\\{\\]\\}\\\\\\|\\;\\:\\'\\\"\\,\\.\\<\\>\\/\\?\\s]+)"
}
